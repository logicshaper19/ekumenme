# Minimal .env file for testing Ekumen Assistant
# Copy this file to .env and replace with your actual API keys when available

# OpenAI Configuration (REQUIRED for basic functionality)
OPENAI_API_KEY=your_openai_api_key_here

# Voice Processing (dummy values for testing)
ELEVENLABS_API_KEY=dummy_key
ELEVENLABS_VOICE_ID=pNInz6obpgDQGcFmaJgB

# Weather APIs (dummy values for testing)
WEATHER_API_KEY=dummy_key
METEO_FRANCE_API_KEY=dummy_key

# Security (REQUIRED - generate a secure random key)
SECRET_KEY=your_very_secure_secret_key_here_make_it_long_and_random_at_least_32_characters

# Database Configuration (defaults to local PostgreSQL)
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/ekumen_assistant
DATABASE_URL_SYNC=postgresql://postgres:postgres@localhost:5432/ekumen_assistant

# Redis Configuration (defaults to local Redis)
REDIS_URL=redis://localhost:6379

# Farm Data APIs (dummy values for testing)
MES_PARCELLES_API_URL=
MES_PARCELLES_API_KEY=dummy_key

# Optional Configuration
DEBUG=true
LOG_LEVEL=INFO
